meta {
  name: Get by last name
  type: http
  seq: 7
}

get {
  url: {{host}}{{port}}/api/employees/findByLastName?lastName={{lastname}}
  body: none
  auth: inherit
}

params:query {
  lastName: {{lastname}}
}

assert {
  res.status: eq 200
  res.body.lastName: eq {{lastname}}
}

settings {
  encodeUrl: true
  timeout: 0
}
